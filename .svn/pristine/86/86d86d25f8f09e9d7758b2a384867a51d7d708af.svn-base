          <td class="memname"><a class="el" href="structsvn__error__t.html">svn_error_t</a>*(* svn_wc_diff_callbacks4_t::dir_added)(<a class="el" href="group__svn__wc__notifications.html#ga86188fa68605f8c57b7849857b1e6543">svn_wc_notify_state_t</a> *state, <a class="el" href="svn__types_8h.html#a22b35baddc4213c688d1bb12feea1024">svn_boolean_t</a> *tree_conflicted, <a class="el" href="svn__types_8h.html#a22b35baddc4213c688d1bb12feea1024">svn_boolean_t</a> *skip, <a class="el" href="svn__types_8h.html#a22b35baddc4213c688d1bb12feea1024">svn_boolean_t</a> *skip_children, const char *path, <a class="el" href="svn__types_8h.html#af16427ed53d30f27da225c56552d2a89">svn_revnum_t</a> rev, const char *copyfrom_path, <a class="el" href="svn__types_8h.html#af16427ed53d30f27da225c56552d2a89">svn_revnum_t</a> copyfrom_revision, void *diff_baton, apr_pool_t *scratch_pool)</td>
