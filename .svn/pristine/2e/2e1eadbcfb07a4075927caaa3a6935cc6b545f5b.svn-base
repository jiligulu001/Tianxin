<a name="l00027"></a>00027 
<a name="l00028"></a>00028 <span class="preprocessor">#ifndef SVN_MERGEINFO_H</span>
<a name="l00029"></a>00029 <span class="preprocessor"></span><span class="preprocessor">#define SVN_MERGEINFO_H</span>
<a name="l00030"></a>00030 <span class="preprocessor"></span>
<a name="l00031"></a>00031 <span class="preprocessor">#include &lt;apr_pools.h&gt;</span>
<a name="l00032"></a>00032 <span class="preprocessor">#include &lt;apr_tables.h&gt;</span>  <span class="comment">/* for apr_array_header_t */</span>
