<div class="line"><a name="l00629"></a><span class="lineno">  629</span>&#160;                                   <span class="keyword">const</span> <span class="keywordtype">char</span> *option_name,</div>
<div class="line"><a name="l00630"></a><span class="lineno">  630</span>&#160;                                   <a class="code" href="svn__types_8h.html#a22b35baddc4213c688d1bb12feea1024">svn_boolean_t</a> default_value);</div>
