<div class="line"><a name="l03729"></a><span class="lineno"> 3729</span>&#160;<a class="code" href="svn__repos_8h.html#a407a6d4ba1171c379dca152a62ede1df">svn_repos_authz_check_access</a>(<a class="code" href="svn__repos_8h.html#a5fce42c096e640743076b32e1f92637a">svn_authz_t</a> *authz,</div>
<div class="line"><a name="l03730"></a><span class="lineno"> 3730</span>&#160;                             <span class="keyword">const</span> <span class="keywordtype">char</span> *repos_name,</div>
<div class="line"><a name="l03731"></a><span class="lineno"> 3731</span>&#160;                             <span class="keyword">const</span> <span class="keywordtype">char</span> *path,</div>
