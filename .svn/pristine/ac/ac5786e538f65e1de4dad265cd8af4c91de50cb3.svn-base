<div class="line"><a name="l00546"></a><span class="lineno">  546</span>&#160;<span class="comment"> */</span></div>
<div class="line"><a name="l00547"></a><span class="lineno">  547</span>&#160;<a class="code" href="structsvn__error__t.html">svn_error_t</a> *</div>
