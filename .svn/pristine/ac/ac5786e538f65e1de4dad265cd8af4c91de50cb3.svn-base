<div class="line"><a name="l01478"></a><span class="lineno"> 1478</span>&#160;                  <span class="keywordtype">void</span> *authz_read_baton,</div>
<div class="line"><a name="l01479"></a><span class="lineno"> 1479</span>&#160;                  apr_pool_t *pool);</div>
