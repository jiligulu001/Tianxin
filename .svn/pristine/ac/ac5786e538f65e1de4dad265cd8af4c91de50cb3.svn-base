<div class="line"><a name="l02103"></a><span class="lineno"> 2103</span>&#160;                         <a class="code" href="svn__types_8h.html#a22b35baddc4213c688d1bb12feea1024">svn_boolean_t</a> include_merged_revisions,</div>
<div class="line"><a name="l02104"></a><span class="lineno"> 2104</span>&#160;                         <a class="code" href="group__svn__repos__authz__callbacks.html#gad36e1f3f846af40f17422af70f08ccf8">svn_repos_authz_func_t</a> authz_read_func,</div>
<div class="line"><a name="l02105"></a><span class="lineno"> 2105</span>&#160;                         <span class="keywordtype">void</span> *authz_read_baton,</div>
<div class="line"><a name="l02106"></a><span class="lineno"> 2106</span>&#160;                         <a class="code" href="group__delta__support.html#ga31e5b4fa524d2ee126ca8f7e7b49eece">svn_file_rev_handler_t</a> handler,</div>
<div class="line"><a name="l02107"></a><span class="lineno"> 2107</span>&#160;                         <span class="keywordtype">void</span> *handler_baton,</div>
<div class="line"><a name="l02108"></a><span class="lineno"> 2108</span>&#160;                         apr_pool_t *pool);</div>
