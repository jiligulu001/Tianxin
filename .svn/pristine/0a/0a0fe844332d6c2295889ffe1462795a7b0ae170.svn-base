<div class="line"><a name="l00327"></a><span class="lineno">  327</span>&#160;                                       apr_off_t original_start,</div>
<div class="line"><a name="l00328"></a><span class="lineno">  328</span>&#160;                                       apr_off_t original_length,</div>
<div class="line"><a name="l00329"></a><span class="lineno">  329</span>&#160;                                       apr_off_t modified_start,</div>
<div class="line"><a name="l00330"></a><span class="lineno">  330</span>&#160;                                       apr_off_t modified_length,</div>
<div class="line"><a name="l00331"></a><span class="lineno">  331</span>&#160;                                       apr_off_t latest_start,</div>
<div class="line"><a name="l00332"></a><span class="lineno">  332</span>&#160;                                       apr_off_t latest_length);</div>
