<div class="line"><a name="l04416"></a><span class="lineno"> 4416</span>&#160;<span class="comment"> */</span></div>
<div class="line"><a name="l04417"></a><span class="lineno"> 4417</span>&#160;<a class="code" href="svn__types_8h.html#aeced35b9b096eb3eb6f3d1af3e685aa1">SVN_DEPRECATED</a></div>
