<div class="line"><a name="l01901"></a><span class="lineno"> 1901</span>&#160;                     <span class="keyword">const</span> <span class="keywordtype">char</span> *path,</div>
<div class="line"><a name="l01902"></a><span class="lineno"> 1902</span>&#160;                     apr_pool_t *pool);</div>
<div class="line"><a name="l01903"></a><span class="lineno"> 1903</span>&#160;<span class="comment"></span></div>
<div class="line"><a name="l01904"></a><span class="lineno"> 1904</span>&#160;<span class="comment">/**</span></div>
<div class="line"><a name="l01905"></a><span class="lineno"> 1905</span>&#160;<span class="comment"> * Callback function type used with svn_fs_try_process_file_contents()</span></div>
<div class="line"><a name="l01906"></a><span class="lineno"> 1906</span>&#160;<span class="comment"> * that delivers the immutable, non-NULL @a contents of @a len bytes.</span></div>
<div class="line"><a name="l01907"></a><span class="lineno"> 1907</span>&#160;<span class="comment"> * @a baton is an implementation-specific closure.</span></div>
<div class="line"><a name="l01908"></a><span class="lineno"> 1908</span>&#160;<span class="comment"> *</span></div>
<div class="line"><a name="l01909"></a><span class="lineno"> 1909</span>&#160;<span class="comment"> * Use @a scratch_pool for allocations.</span></div>
<div class="line"><a name="l01910"></a><span class="lineno"> 1910</span>&#160;<span class="comment"> *</span></div>
<div class="line"><a name="l01911"></a><span class="lineno"> 1911</span>&#160;<span class="comment"> * @since New in 1.8.</span></div>
<div class="line"><a name="l01912"></a><span class="lineno"> 1912</span>&#160;<span class="comment"> */</span></div>
<div class="line"><a name="l01913"></a><span class="lineno"> 1913</span>&#160;<span class="keyword">typedef</span> <a class="code" href="structsvn__error__t.html">svn_error_t</a> *</div>
